{
  "name": "@aureooms/js-codec",
  "description": "Codecs for JavaScript",
  "version": "3.0.0",
  "author": "aureooms",
  "ava": {
    "require": [
      "@babel/polyfill",
      "@babel/register"
    ],
    "files": [
      "test/src/**/*"
    ],
    "timeout": "1m"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "env": {
      "development": {
        "sourceMaps": "inline"
      }
    }
  },
  "bugs": {
    "url": "https://github.com/aureooms/js-codec/issues"
  },
  "dependencies": {
    "@aureooms/js-error": "^4.0.0"
  },
  "devDependencies": {
    "@babel/cli": "7.11.6",
    "@babel/core": "7.11.6",
    "@babel/polyfill": "7.11.5",
    "@babel/preset-env": "7.11.5",
    "@babel/register": "7.11.5",
    "ava": "3.12.1",
    "coveralls": "3.1.0",
    "esdoc": "1.1.0",
    "esdoc-inject-script-plugin": "1.0.0",
    "esdoc-inject-style-plugin": "1.0.0",
    "esdoc-standard-plugin": "1.0.0",
    "np": "6.5.0",
    "nyc": "15.1.0"
  },
  "files": [
    "lib"
  ],
  "homepage": "https://aureooms.github.io/js-codec",
  "keywords": [
    "codec",
    "decode",
    "encode"
  ],
  "license": "AGPL-3.0",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/aureooms/js-codec"
  },
  "scripts": {
    "build": "rm -rf lib && babel src -d lib",
    "cover": "nyc --reporter=lcov npm test",
    "esdoc": "esdoc",
    "prepare": "npm run build",
    "release": "np",
    "test": "ava"
  }
}
